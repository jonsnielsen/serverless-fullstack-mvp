{
  "stage": "${opt:stage, 'dev'}",
  "region": "<REGION>",
  "appName": "<APP_NAME>",
  "awsProfile": "<AWS_PROFILE>",
  "endpoints": {
    "dev": ["http://localhost:3000", "<ENDPOINT>"],
    "prod": ["<ENDPOINT>"]
  },
  "cognitoIssuer": {
    "dev": "https://cognito-idp.<REGION>.amazonaws.com/<USER_POOL_ID>",
    "prod": "https://cognito-idp.<REGION>.amazonaws.com/<USER_POOL_ID>"
  }
}
